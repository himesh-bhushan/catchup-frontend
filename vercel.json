{
  "version": 2,
  "outputDirectory": "build",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "catchup.page" }
      ],
      "destination": "https://www.catchup.page/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}